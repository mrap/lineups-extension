{
  "name": "Better Lineups",
  "description": "Features for lineups.com",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [
    {
      "css": ["styles.css"],
      "js": ["ext.js"],
      "matches": ["https://www.lineups.com/*"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "/images/get_started16.png",
      "32": "/images/get_started32.png",
      "48": "/images/get_started48.png",
      "128": "/images/get_started128.png"
    }
  },
  "icons": {
    "16": "/images/get_started16.png",
    "32": "/images/get_started32.png",
    "48": "/images/get_started48.png",
    "128": "/images/get_started128.png"
  }
}
